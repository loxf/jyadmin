<div id="play_video_div" style="width:100%; height:auto;"></div>

<script type="text/javascript"> (function () {
    var option = {
        "auto_play": "0",
        "file_id": "${outVideoId}",
        "app_id": "${appId}",
        "width": 650,
        "height": "100%",
        "https": 1
